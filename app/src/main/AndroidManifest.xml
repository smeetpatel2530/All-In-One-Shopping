<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smeet.allinoneshopping">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:usesCleartextTraffic="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
<!--        <activity android:name="com.smeet.allinoneshopping.AboutApp"></activity>-->
        <activity android:name="com.smeet.allinoneshopping.SplashActivity" />
        <activity
            android:name="com.smeet.allinoneshopping.eBay"
            android:parentActivityName="com.smeet.allinoneshopping.SplashActivity" />
        <activity
            android:name="com.smeet.allinoneshopping.shopclue"
            android:parentActivityName="com.smeet.allinoneshopping.SplashActivity" />
        <activity
            android:name="com.smeet.allinoneshopping.paytm"
            android:parentActivityName="com.smeet.allinoneshopping.SplashActivity" />
        <activity
            android:name="com.smeet.allinoneshopping.snapdeal"
            android:parentActivityName="com.smeet.allinoneshopping.SplashActivity" />
        <activity
            android:name="com.smeet.allinoneshopping.myntra"
            android:parentActivityName="com.smeet.allinoneshopping.SplashActivity" />
        <activity
            android:name="com.smeet.allinoneshopping.flipkart"
            android:parentActivityName="com.smeet.allinoneshopping.SplashActivity" />
        <activity
            android:name="com.smeet.allinoneshopping.amazon"
            android:parentActivityName="com.smeet.allinoneshopping.SplashActivity" />
        <activity android:name="com.smeet.allinoneshopping.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>